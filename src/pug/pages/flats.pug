block content
  include ../includes/breadcrumbs.pug   
  .mobile-sort-list2.js-sort-list
    .contant 
      .mobile-filter-close 
          h4 Сортувати
          +svg('close-select').js-close-sort
      ul.sort-list 
        li.sort-item.check Спочатку з більшою площою
        li.sort-item Спочатку з меншою площою
        li.sort-item За строком сдачі
  .planing-filter 
    h2.our-projects__title Квартири
    .mobile-filter-btn
      .mobile-btn-nav 
      button.btn-mobile-filter.js-open-filter
        +svg('filter')
        span Фільтр
      .mobile-btn-sort.js-sort-btn
    .mobile-filter.js-mobile-filter
      .mobile-filter-close 
        h4 Фільтр
        +svg('close-select').js-close-filter
      .planing-filter__container 
        .planing-filt__complex
          select(class="select" name="connection")
            option(disabled) Будь-який
            option(value="Continent-Ray") Continent Ray
            option(value="Continent-Black") Continent Black
            option(value="Continent-Ray-2.1") Continent Ray 2.1
          .marker-text Житловий комплекс
        .planing-filt__range
          .planing-filt__range-container
            input(type="text" class="js-range-slider" name="my_range" value="")
          .marker-text Площа м²
        .planing-filt__range.number-flats
          .planing-filt__range-container
            .checkboxes__rooms
              input(class="checkbox__room filter__checkbox" id="checkbox__room1" type="checkbox" value="1")
              label(for="checkbox__room1" class="checkbox__text") 1
              input(class="checkbox__room filter__checkbox" id="checkbox__room2" type="checkbox" value="2")
              label(for="checkbox__room2" class="checkbox__text") 2
              input(class="checkbox__room filter__checkbox" id="checkbox__room3" type="checkbox" value="3")
              label(for="checkbox__room3" class="checkbox__text") 3
              input(class="checkbox__room filter__checkbox" id="checkbox__room4" type="checkbox" value="4")
              label(for="checkbox__room4" class="checkbox__text") 4
          .marker-text Кількість кімнат

        .planing-filt__deadline
          select(class="select" name="connection")
            option(disabled) Будь-який 
            option(value="2022") ІІІ кв. 2022
            option(value="2023") ІІІ кв. 2023
            option(value="2024") ІІІ кв. 2024
          .marker-text Строк здачі
        .planing-filt__range.checkbox__item
          .checkbox__block-item
            input(class="checkbox__room filter__checkbox" id="checkbox__two-level" type="checkbox" value="")
            label(for="checkbox__two-level" class="checkbox__text") 
            .checkbox__block-name Дворівневі квартири
          .checkbox__block-item
            input(class="checkbox__room filter__checkbox" id="checkbox__terrace" type="checkbox" value="")
            label(for="checkbox__terrace" class="checkbox__text")
            .checkbox__block-name  Квартири з терасою
        .planing-filt__deadline.planing-filter__close
          button.reset-filter 
            +svg('close-select')
            span Скинути фільтр

      .planing-filter__close 
        .result-filter Знайдено 
          .number 153 
          | планування
          +svg('arrow')
      .planing-filter__close
        button.realty.nav__call-btn Переглянути 
          span.number 15 
          | квартир

    .planing-filter__last-session 
      p Відновити минулий пошук? Continent Black, від 60 до 70 м² 
      button.reset-filter 
        +svg('close-select')


  section.planing(id="container")
    .planing__nav
      
      .tab.buttons
        button.tablinks.tabs__nav-btn.grid.js-btn-grid.active Картками
        button.tablinks.tabs__nav-btn.list.js-btn-list Cписком 
      select(class="select" name="connection")
        option(disabled) Спочатку з більшою площою
        option(value="big-area") Спочатку з більшою площою
        option(value="smaller-area") Спочатку з меншою площою
        option(value="dedline") За строком сдачі
    .list-title.js-sort-title
      p.planing__img План
      p.planing__title Житловий квартал
      p.square Площа 
      p.square жила Площа
      p.flats Кімнати 
      p.deadlines Строки здачі
      p.price Вартість
      p.favorites
    ul.planing__list.grid
      li.planing__item
        a(href="single-flats.html").planing__link
          .price
            span.new-price 1 200 000 ₴
          .favorite-star
            .add-favourite
              label.favourite-label
                input(type="checkbox")
                +svg('star-circle')
          .planing__img 
            img(src='./assets/images/planing/planing1.jpg')
          h4.planing__title
            span ЖК: 
            | Сontinent Black
          .flat-planing-container
            .square 
              +svg('space')
              .square-description 
                p.title Площа:
                p.value 48 м²
            .square 
              +svg('living_space')
              .square-description 
                p.title жила Площа:
                p.value 36 м² 
            .square 
              +svg('room')
              .square-description 
                p.title Кімнат:
                p.value 1
          .chekbox__container
