block content
  include ../includes/breadcrumbs.pug
  .busha-text
    h2.our-projects__title Комерційні приміщення
  //- .commerce-content 
  //-   .commerce-content__img 
  //-     img(src="./assets/images/commerce.jpg", alt="")
  //-   .commerce-content__text
  //-     ul.commerce-benefits 
  //-       li.commerce-benefits__list 
  section.infrastructure-benefits 
    .infrastructure-benefits__img
      img(src='./assets/images/commerce.jpg')
    ul.infrastructure-benefits__list 
      each v, i in benefits
        li.infrastructure-benefits__item 
          .num= i + 1
          .title= v[0]
          .text= v[1]

  .planing-filter 
    h2.our-projects__title Планування
    .planing-filter__container 
      .planing-filt__complex
        select(class="select" name="connection")
          option(disabled) Спочатку: Continent Ray
          option(value="Continent-Ray") Continent Ray
          option(value="Continent-Black") Continent Black
          option(value="Continent-Ray-2.1") Continent Ray 2.1
        .marker-text Житловий комплекс
      .planing-filt__range
        input(type="text" class="js-range-slider" name="my_range" value="" 
        
      
       )
        .marker-text Площа м²
      .planing-filt__deadline
        select(class="select" name="connection")
          option(disabled) Будь-який 
          option(value="2022") ІІІ кв. 2022
          option(value="2023") ІІІ кв. 2023
          option(value="2024") ІІІ кв. 2024
        .marker-text Строк здачі
  
  section.planing(id="container")
    .planing__nav 
      .tab.buttons
        button.tablinks.tabs__nav-btn.grid.js-btn-grid.active Картками
        button.tablinks.tabs__nav-btn.list.js-btn-list Cписком 
      select(class="select" name="connection")
        option(disabled) Спочатку: Continent Ray
